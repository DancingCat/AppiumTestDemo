<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="appiunTestDemo">
  <test name="iPhone 8">
    <parameter name="deviceName" value="iPhone 8"/>
    <parameter name="platformVersion" value="9.3"/>
    <parameter name="port" value="9003"/>
    <groups>
      <run>
        <include name="smoke"/>
        <include name="wode"/>
        <exclude name="shangcheng"/>
      </run>
    </groups>
    <packages>
      <package name="com.cpeoc.appiunTestDemo.test.mine">
        <exclude name="com.cpeoc.appiunTestDemo.test.mine"/>
      </package>
    </packages>
  </test>
  <test name="iPhone 9">
    <parameter name="deviceName" value="iPhone 9"/>
    <parameter name="platformVersion" value="10.3"/>
    <parameter name="port" value="9004"/>
    <groups>
      <run>
        <include name="smoke"/>
        <include name="wode"/>
        <exclude name="shangcheng"/>
      </run>
    </groups>
    <packages>
      <package name="com.cpeoc.appiunTestDemo.test.xx">
        <exclude name="com.cpeoc.appiunTestDemo.test.mine"/>
      </package>
    </packages>
  </test>
</suite>
